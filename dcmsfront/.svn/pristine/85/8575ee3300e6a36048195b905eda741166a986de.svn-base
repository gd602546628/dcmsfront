package com.kissdental.weixin.vo;

import java.util.Date;

/**
 * Created by dartagnan on 2018/1/6.
 */
public class CustomerVO {

    private String id;
    private String code;		// 病历号
    private String name;		// 姓名
    private String sex;		// 性别：1男，2女
    private String phone;
    private String referrerQrcode;  //推荐二维码
    private String outpatientQrcode;//就诊二维码

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getSex() {
        return sex;
    }

    public void setSex(String sex) {
        this.sex = sex;
    }

    public String getPhone() {
        return phone;
    }

    public void setPhone(String phone) {
        this.phone = phone;
    }

    public String getReferrerQrcode() {
        return referrerQrcode;
    }

    public void setReferrerQrcode(String referrerQrcode) {
        this.referrerQrcode = referrerQrcode;
    }

    public String getOutpatientQrcode() {
        return outpatientQrcode;
    }

    public void setOutpatientQrcode(String outpatientQrcode) {
        this.outpatientQrcode = outpatientQrcode;
    }
}
